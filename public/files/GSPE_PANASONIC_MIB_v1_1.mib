PANASONIC-BATTERY-DCB105ZK-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
    Integer32, Unsigned32, Counter32, Gauge32, TimeTicks
        FROM SNMPv2-SMI
    DisplayString, TruthValue
        FROM SNMPv2-TC
    MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
        FROM SNMPv2-CONF;

PT-GSPE MODULE-IDENTITY
    LAST-UPDATED "202510010000Z"
    ORGANIZATION "BMS Battery Management System"
    CONTACT-INFO
        "Contact: BMS Support Team
         Email: support@bms.local
         Description: Panasonic DCB105ZK Battery Management System SNMP MIB"
    DESCRIPTION
        "This MIB module defines objects for monitoring and managing
         Panasonic DCB105ZK lithium-ion battery packs through MODBUS RTU
         protocol via SNMP interface. Supports 13S configuration with
         comprehensive BMS protection monitoring.

         Version 2.1 - Multi-equipment support with dynamic OID structure
         - Structure: 12.<equipment_id>.<parameter_id>
         - Equipment 1: Battery Charger
         - Equipment 2: Panasonic DCB105ZK Battery (139 parameters)
         - All status, warning, alarm, and error flags as individual OIDs
         - Dual value format: scaled integers + real floating point
         - Operation mode strings and text descriptions
         - Enhanced trap definitions for individual flag monitoring"
    REVISION "202510010000Z"
    DESCRIPTION "Version 2.1 - Multi-equipment OID structure"
    REVISION "202509290000Z"
    DESCRIPTION "Initial version for Panasonic DCB105ZK battery system"
    ::= { enterprises 10000 }

-- Enterprise and organizational structure
PanasonicBattery   OBJECT IDENTIFIER ::= { PT-GSPE 12 }
Config             OBJECT IDENTIFIER ::= { PanasonicBattery 5 }
Equipment          OBJECT IDENTIFIER ::= { PanasonicBattery 12 }
ThresholdTraps     OBJECT IDENTIFIER ::= { PanasonicBattery 13 }
BitmaskTraps       OBJECT IDENTIFIER ::= { PanasonicBattery 14 }
HealthTraps        OBJECT IDENTIFIER ::= { PanasonicBattery 15 }
ErrorTraps         OBJECT IDENTIFIER ::= { PanasonicBattery 16 }
AlarmTraps         OBJECT IDENTIFIER ::= { PanasonicBattery 17 }
WarningTraps       OBJECT IDENTIFIER ::= { PanasonicBattery 18 }
StatusTraps        OBJECT IDENTIFIER ::= { PanasonicBattery 19 }

