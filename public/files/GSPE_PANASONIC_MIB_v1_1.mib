    OBJECTS { panasonicMaxCellTemperature, panasonicMaxCellTemperatureReal, panasonicPackVoltage, panasonicSOC, deviceName, siteName }
    STATUS  current
    DESCRIPTION "Battery temperature above threshold alarm"
    ::= { ThresholdTraps 4 }

batteryLowTemp NOTIFICATION-TYPE
    OBJECTS { panasonicMinCellTemperature, panasonicMinCellTemperatureReal, panasonicPackVoltage, panasonicSOC, deviceName, siteName }
