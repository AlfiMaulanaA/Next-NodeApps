-- ========== Threshold Alarms (13.x.0) ==========
batteryLowVoltage NOTIFICATION-TYPE
    OBJECTS { panasonicPackVoltage, panasonicSOC, panasonicPackVoltageReal, panasonicSOCReal, deviceName, siteName }
    STATUS  current
    DESCRIPTION "Battery pack voltage below threshold alarm"
    ::= { ThresholdTraps 1 }

batteryHighVoltage NOTIFICATION-TYPE
    OBJECTS { panasonicPackVoltage, panasonicSOC, panasonicPackVoltageReal, panasonicSOCReal, deviceName, siteName }
    STATUS  current
    DESCRIPTION "Battery pack voltage above threshold alarm"
    ::= { ThresholdTraps 2 }

batteryLowSOC NOTIFICATION-TYPE
    OBJECTS { panasonicSOC, panasonicPackVoltage, panasonicSOCReal, panasonicPackVoltageReal, deviceName, siteName }
    STATUS  current
    DESCRIPTION "Battery State of Charge below threshold alarm"
    ::= { ThresholdTraps 3 }

