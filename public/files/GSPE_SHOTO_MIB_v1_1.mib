-- Equipment Objects - Battery Data (matches MQTT payload naming)
batteryData    OBJECT IDENTIFIER ::= { Equipment 1 }

-- Battery Main Parameters (1-14) - Naming matches payload keys
packVoltage OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "centi-volts"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Pack Voltage - Battery pack voltage in centi-volts (divide by 100 for actual volts)
                 MQTT Payload Key: 'Pack Voltage'"
    ::= { batteryData 1 }

current OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "centi-amperes"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Current - Battery current in centi-amperes (divide by 100 for actual amperes)
                 MQTT Payload Key: 'Current'"
    ::= { batteryData 2 }

remainingCapacity OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "centi-ampere-hours"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Remaining capacity - Remaining capacity in centi-ampere-hours
                 MQTT Payload Key: 'Remaining capacity'"
    ::= { batteryData 3 }

totalCapacity OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "centi-ampere-hours"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Total Capacity - Total capacity in centi-ampere-hours
                 MQTT Payload Key: 'Total Capacity'"
    ::= { batteryData 4 }

totalDischargeCapacity OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "ampere-hours"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Total Discharge Capacity - Total discharge capacity in ampere-hours
                 MQTT Payload Key: 'Total Discharge Capacity'"
    ::= { batteryData 5 }

soc OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "deci-percent"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "SOC - State of Charge in deci-percent (divide by 10 for actual percentage)
                 MQTT Payload Key: 'SOC'"
    ::= { batteryData 6 }

soh OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "deci-percent"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "SOH - State of Health in deci-percent (divide by 10 for actual percentage)
                 MQTT Payload Key: 'SOH'"
    ::= { batteryData 7 }

cycle OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Cycle - Battery cycle count
                 MQTT Payload Key: 'Cycle'"
    ::= { batteryData 8 }

averagOfCellVotage OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "milli-volts"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Averag of Cell Votage - Average cell voltage in milli-volts (divide by 1000 for volts)
                 MQTT Payload Key: 'Averag of Cell Votage' (note: typo in original)"
    ::= { batteryData 9 }

maxCellVoltage OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "milli-volts"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Max Cell Voltage - Maximum cell voltage in milli-volts
                 MQTT Payload Key: 'Max Cell Voltage'"
    ::= { batteryData 10 }

minCellVoltage OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "milli-volts"
